  "keywords": ["decoration", "decorator", "symfony", "bundle"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "~2.4",
    "symfony/twig-bundle": "~2.4",

    "cleentfaar/decorator": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1"
  },
  "autoload": {
    "psr-0": {
      "CL\\Bundle\\DecoratorBundle": ""
    }
